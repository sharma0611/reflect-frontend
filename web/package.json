{
    "name": "reflect_web",
    "version": "0.0.1",
    "main": "src/index.html",
    "scripts": {
        "start": "parcel src/index.html",
        "build": "parcel build src/index.html",
        "deploy": "parcel build src/index.html && firebase deploy --only hosting",
        "deployAll": "parcel build src/index.html && firebase deploy",
        "local": "LOCAL=true parcel src/index.html",
        "shell": "firebase functions:shell",
        "emulators": "firebase emulators:start",
        "importIndexes": "firebase firestore:indexes > firestore.indexes.json",
        "importRules": "open \"https://console.firebase.google.com/u/1/project/reflect-78453/database/firestore/rules\" && code firestore.rules",
        "import": "yarn run importIndexes && yarn run importRules"
    },
    "dependencies": {
        "@reach/router": "^1.3.3",
        "firebase": "^7.9.3",
        "lodash-es": "^4.17.15",
        "react": "^16.13.0",
        "react-dom": "^16.13.0"
    },
    "devDependencies": {
        "@babel/core": "^7.8.6",
        "@babel/preset-env": "^7.8.6",
        "@babel/preset-react": "^7.8.3",
        "firebase-tools": "^7.14.0",
        "parcel-bundler": "^1.12.4"
    }
}
